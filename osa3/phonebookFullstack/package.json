{
  "name": "phonebookbacked",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "client": "npm run start --prefix client",
    "server": "babel-node ./src/bin/www",
    "dev": " NODE_ENV=development npm-run-all server",
    "startAll": "concurrently --kill-others-on-fail \"npm run dev\" \"npm run client\"",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "watch": "nodemon"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "express": "~4.16.1",
    "fs": "0.0.1-security",
    "http-errors": "~1.6.3",
    "mongoose": "^5.7.1",
    "morgan": "^1.9.1",
    "normalize-port": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "pug": "^2.0.4",
    "rimraf": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.1"
  }
}
